          <apply>
            <minus/>
            <apply>
              <times/>
              <ci>MREABS_k</ci>
              <ci>m2_ext</ci>
              <ci>Vlumen</ci>
            </apply>
            <apply>
              <times/>
              <ci>MEXC_k</ci>
              <ci>m2_lumen</ci>
              <ci>Vlumen</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>

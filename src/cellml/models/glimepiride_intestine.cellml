        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>gli_lumen</ci>
        </apply>
        <apply>
          <times/>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless">1</cn>
            <ci>Vlumen</ci>

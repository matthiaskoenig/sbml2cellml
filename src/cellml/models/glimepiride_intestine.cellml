            <divide/>
            <cn cellml:units="dimensionless">1</cn>
            <ci>Vext</ci>
          </apply>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci>MREABS_k</ci>
              <ci>m2_ext</ci>
              <ci>Vlumen</ci>
            </apply>
          </apply>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>m2_lumen</ci>
        </apply>
        <apply>
          <times/>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless">1</cn>
